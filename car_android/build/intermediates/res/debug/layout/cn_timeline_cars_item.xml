<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="15dp"
    android:background="@color/cn_transparent"
    android:orientation="vertical"
    android:paddingRight="20dp">

    <RelativeLayout
        android:id="@+id/rlCars"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/cn_transparent">
        <ImageView
            android:id="@+id/ivTimePoint"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="24dp"
            android:layout_marginStart="24dp"
            android:src="@drawable/cn_timeline_point"/>

        <LinearLayout
            android:id="@+id/llTimeCarsInfo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:layout_marginStart="15dp"
            android:layout_toRightOf="@+id/ivTimePoint"
            android:background="@color/cn_common_cars_bgcolor"
            android:orientation="horizontal"
            android:paddingBottom="15dp"
            android:paddingLeft="5dp"
            android:paddingRight="5dp"
            android:paddingTop="15dp">
            <com.kplus.car.widget.RegularTextView
                android:id="@+id/tvCarsLicence"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="5dp"
                android:layout_marginStart="5dp"
                android:gravity="center_vertical"
                android:text="@null"
                android:textColor="@color/cn_white"
                android:textSize="16sp"/>

            <com.kplus.car.widget.RegularTextView
                android:id="@+id/tvCarsModel"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="10dp"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:singleLine="true"
                android:text="@null"
                android:textColor="@color/cn_white"
                android:textSize="16sp"/>

            <com.kplus.car.widget.RegularTextView
                android:id="@+id/tvCarsColor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="5dp"
                android:gravity="center_vertical"
                android:text="@null"
                android:textColor="@color/cn_white"
                android:textSize="16sp"/>
        </LinearLayout>
    </RelativeLayout>

    <View
        android:id="@+id/viewBottom"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_below="@+id/rlCars"
        android:background="@color/cn_transparent"
        android:visibility="gone"/>

</RelativeLayout><!-- From: file:/D:/workspace/car_android/Car_washing/src/main/res/layout/cn_timeline_cars_item.xml --><!-- From: file:/D:/workspace/car_android/build/intermediates/exploded-aar/car_android/Car_washing/unspecified/res/layout/cn_timeline_cars_item.xml -->