<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@+id/llMyOrder"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/layout_item_bg_selector"
    android:orientation="vertical">
    <View
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:background="@color/cn_space_color"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="2px"
        android:background="@color/cn_line_color"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="20dp"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:orientation="vertical">

            <com.kplus.car.widget.RegularTextView
                android:id="@+id/tvServiceItem"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:singleLine="true"
                android:text="@null"
                android:textColor="@color/cn_common_cars_bgcolor"
                android:textSize="16sp"/>

            <com.kplus.car.widget.RegularTextView
                android:id="@+id/tvOrderTime"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginTop="5dp"
                android:gravity="center_vertical"
                android:singleLine="true"
                android:text="@null"
                android:textColor="@color/cn_font_color"
                android:textSize="16sp"/>
        </LinearLayout>

        <com.kplus.car.widget.RegularTextView
            android:id="@+id/tvOrderStatu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right|center_vertical"
            android:layout_marginLeft="15dp"
            android:gravity="right|center_vertical"
            android:text="@null"
            android:textColor="@color/cn_oranger_color"
            android:textSize="16sp"/>
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="2px"
        android:layout_marginTop="15dp"
        android:background="@color/cn_line_color"/>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="20dp"
        android:gravity="center_vertical"
        android:orientation="vertical">

        <com.kplus.car.widget.RegularTextView
            android:id="@+id/tvCarInfo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:singleLine="true"
            android:text="@null"
            android:textColor="@color/cn_font_color"
            android:textSize="16sp"/>

        <com.kplus.car.widget.RegularTextView
            android:id="@+id/tvCarLocation"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginTop="5dp"
            android:gravity="center_vertical"
            android:singleLine="true"
            android:text="@null"
            android:textColor="@color/cn_font_color"
            android:textSize="16sp"/>
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="2px"
        android:layout_marginTop="15dp"
        android:background="@color/cn_line_color"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical|right"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="10dp"
        android:gravity="center_vertical|right"
        android:orientation="horizontal">
        <com.kplus.car.widget.RegularTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:gravity="center_vertical"
            android:text="@string/cn_realPay"
            android:textColor="@color/cn_font_color"
            android:textSize="16sp"/>
        <com.kplus.car.widget.RegularTextView
            android:id="@+id/tvPrice"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:gravity="center_vertical"
            android:text="@null"
            android:textColor="@color/cn_common_cars_bgcolor"
            android:textSize="16sp"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/llActionCloseOrPay"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical|right"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:gravity="center_vertical|right"
        android:orientation="horizontal"
        android:paddingBottom="10dp"
        android:visibility="visible">
        <Button
            android:id="@+id/btnCloseOrder"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="10dp"
            android:background="@drawable/btn_my_close_order_selector"
            android:gravity="center"
            android:paddingBottom="10dp"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:paddingTop="10dp"
            android:text="@string/cn_order_cancel"
            android:textColor="@color/cn_common_cars_bgcolor"
            android:textSize="16sp"
            android:visibility="gone"/>

        <Button
            android:id="@+id/btnOrderPay"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="10dp"
            android:background="@drawable/btn_my_pay_order_selector"
            android:gravity="center"
            android:paddingBottom="10dp"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:paddingTop="10dp"
            android:text="@string/cn_pay"
            android:textColor="@color/cn_oranger_color"
            android:textSize="16sp"
            android:visibility="gone"/>

        <Button
            android:id="@+id/btnOrderEvaluate"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="10dp"
            android:background="@drawable/btn_my_pay_order_selector"
            android:gravity="center"
            android:paddingBottom="10dp"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:paddingTop="10dp"
            android:text="@string/cn_evalute"
            android:textColor="@color/cn_oranger_color"
            android:textSize="16sp"
            android:visibility="visible"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/llLoadMore"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:orientation="vertical"
        android:visibility="gone">
        <View
            android:layout_width="match_parent"
            android:layout_height="2px"
            android:background="@color/cn_line_color"/>
        <com.kplus.car.widget.RegularTextView
            android:id="@+id/tvLoadMore"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:padding="16dp"
            android:text="@null"
            android:textSize="14sp"/>
    </LinearLayout>
</LinearLayout><!-- From: file:/D:/workspace/car_android/Car_washing/src/main/res/layout/cn_orders_list_item.xml --><!-- From: file:/D:/workspace/car_android/build/intermediates/exploded-aar/car_android/Car_washing/unspecified/res/layout/cn_orders_list_item.xml -->