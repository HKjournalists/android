<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 支付按钮的selector -->

    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/cn_press_color"/>
            <stroke android:width="1dp" android:color="@color/cn_oranger_color"/>
            <corners android:radius="5dp"/>
        </shape>

    </item>
    <item android:state_pressed="false">
        <shape>
            <solid android:color="@color/cn_white"/>
            <stroke android:width="1dp" android:color="@color/cn_oranger_color"/>
            <corners android:radius="5dp"/>
        </shape>

    </item>
    <item>
        <shape>
            <solid android:color="@color/cn_white"/>
            <stroke android:width="1dp" android:color="@color/cn_oranger_color"/>
            <corners android:radius="5dp"/>
        </shape>

    </item>

</selector>
